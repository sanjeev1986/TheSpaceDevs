<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/browse"
    app:startDestination="@+id/upcomingLaunchesFragment">

    <fragment
        android:id="@+id/upcomingLaunchesFragment"
        android:name="com.sample.thespacedevs.upcoming.UpcomingLaunchesFragment"
        android:label="Upcoming launches"
        tools:layout="@layout/fragment_upcoming_launches">
        <action
            android:id="@+id/action_upcomingLaunchesFragment_to_launchDetailsFragment"
            app:destination="@id/launchDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpToInclusive="@id/upcomingLaunchesFragment" />
    </fragment>

    <fragment
        android:id="@+id/launchDetailsFragment"
        android:name="com.sample.thespacedevs.details.LaunchDetailsFragment"
        app:popUpToInclusive="@id/upcomingLaunchesFragment"
        tools:layout="@layout/fragment_launch_details">
        <argument
            android:name="result"
            app:argType="com.sample.services.launch.Results"
            app:nullable="false" />
    </fragment>
</navigation>